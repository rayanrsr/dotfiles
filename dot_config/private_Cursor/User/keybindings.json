// Place your key bindings in this file to override the defaults
[
  {
    "key": "g g",
    "command": "cursorTop",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "ctrl+i",
    "command": "composerMode.agent"
  },
  {
    "key": "g d",
    "command": "editor.action.revealDefinition",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "g D",
    "command": "editor.action.revealDefinitionAside",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "g r",
    "command": "editor.action.goToReferences",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "ctrl+p",
    "command": "workbench.action.quickOpen",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "ctrl+shift+p",
    "command": "workbench.action.showCommands",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "ctrl+shift+f",
    "command": "workbench.action.findInFiles",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "space p v",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "space t",
    "command": "workbench.action.closePanel",
    "when": "vim.active && vim.mode == 'Normal' && !terminalFocus"
  },
  {
    "key": "ctrl+j",
    "command": "workbench.action.terminal.focus",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "q",
    "command": "workbench.action.toggleSidebarVisibility",
    "when": "sideBarFocus"
  },
  {
    "key": "ctrl+l",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "explorerViewletFocus || filesExplorerFocus || sideBarFocus"
  },
  {
    "key": "ctrl+k",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "terminalFocus"
  },
  // {
  //   "key": "/",
  //   "command": "actions.find",
  //   "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  // },
  {
    "key": "space a a",
    "command": "composerMode.chat",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "space c",
    "command": "workbench.action.chat.newChat",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal' && chatIsEnabled && inChat && chatLocation != 'editing-session'"
  },
  // {
  //   "key": "space a k",
  //   "command": "aipopup.action.modal.generate",
  //   "when": "editorTextFocus && vim.active && vim.mode == 'Normal' && editorFocus && !composerBarIsVisible"
  // },
  {
    "key": "ctrl+l",
    "command": "-workbench.action.chat.newChat",
    "when": "chatIsEnabled && inChat && chatLocation != 'editing-session'"
  },
  {
    "key": "ctrl+l",
    "command": "-workbench.action.chat.newEditSession",
    "when": "chatEditingParticipantRegistered && chatIsEnabled && inChat && chatLocation == 'editing-session'"
  },
  {
    "key": "ctrl+l",
    "command": "-aichat.newchataction"
  },
  // {
  //   "key": "ctrl+l",
  //   "command": "-composer.sendToAgent",
  //   "when": "editorHasPromptBar && editorPromptBarFocused"
  // },
  {
    "key": "space c c",
    "command": "workbench.action.chat.toggle",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "ctrl+l",
    "command": "aichat.newchataction",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "inChat && !searchViewletVisible"
  },
  {
    "key": "ctrl+h",
    "command": "-editor.action.startFindReplaceAction",
    "when": "inChat"
  },
  {
    "key": "ctrl+h",
    "command": "-extension.vim_ctrl+h",
    "when": "editorTextFocus && vim.active && vim.use<C-h> && !inDebugRepl"
  },
  {
    "key": "ctrl+h",
    "command": "-editor.action.startFindReplaceAction",
    "when": "editorFocus || editorIsOpen"
  },
  {
    "key": "ctrl+h",
    "command": "-editor.action.simpleInlineDiffs.rejectAll",
    "when": "editorTextFocus && hasDisplayedSimpleDiff"
  },
  {
    "key": "ctrl+h",
    "command": "-workbench.action.toggleCenteredLayout",
    "when": "inChat"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.focusSideBar",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "ctrl+h",
    "command": "workbench.action.focusActiveEditorGroup",
    "when": "!editorTextFocus && !editorFocus"
  },
  {
    "key": "space a",
    "command": "vscode-harpoon.addEditor",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "space e",
    "command": "vscode-harpoon.editEditors",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "space 1",
    "command": "vscode-harpoon.gotoEditor1",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "space 2",
    "command": "vscode-harpoon.gotoEditor2",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "space 3",
    "command": "vscode-harpoon.gotoEditor3",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "space 4",
    "command": "vscode-harpoon.gotoEditor4",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  },
  {
    "key": "space 5",
    "command": "vscode-harpoon.gotoEditor5",
    "when": "editorTextFocus && vim.active && vim.mode == 'Normal'"
  }
]